{
    "name": "castro/shadblade",
    "description": "Tailwind-based blade components",
    "type": "library",
    "require": {
        "php": ">=8.3",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/view": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Castro\\Shadblade\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Castro\\Shadblade\\ShadbladeServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
